a1 = [.536719 .754504 .879031 .943429 .973677 .988320 .994890];
a2 = [.411163 .667223 .831419 .919661 .962250 .982993 .992425];
a3 = [.342856 .565039 .742610 .851091 .915931 .953056 .973713];

b1 = 1.-a1;
b2 = 1.-a2;
b3 = 1.-a3;

b1 = [0.463281 0.245496 0.120969 0.056571 0.026323 0.011680 0.005110];
b2 = [0.588837 0.332777 0.168581 0.080339 0.037750 0.017007 0.007575];
b3 = [0.657144 0.434961 0.257390 0.148909 0.084069 0.046944 0.026287];

b1 = [0.4633  0.2455  0.1210  0.0566  0.0263  0.0117  0.0051];
b2 = [0.5888  0.3328  0.1686  0.0803  0.0377  0.0170  0.0076];
b3 = [0.6571  0.4350  0.2574  0.1489  0.0841  0.0469  0.0263];

for i = 1:6
    c1(i) = b1(i)/b1(i+1);
    c2(i) = b2(i)/b2(i+1);
    c3(i) = b3(i)/b3(i+1);
end

c1 = [1.8871    2.0294    2.1384    2.1491    2.2537    2.2857];
c2 = [1.7695    1.9740    2.0984    2.1282    2.2197    2.2451];
c3 = [1.5108    1.6899    1.7285    1.7713    1.7908    1.7858];

 x=[1 2 3 4 5 6 7];
 plot(x,a1,'*',x,a2,'--',x,a3,'-')
 semilogy(x,a1,'*',x,a2,'--',x,a3,'-')
 plot(x,b1,'*',x,b2,'--',x,b3,'-')    
 semilogy(x,b1,'*',x,b2,'--',x,b3,'-')
 y=[1 2 3 4 5 6];
 plot(y,c1,'*',y,c2,'--',y,c3,'-')
 semilogy(y,c1,'*',y,c2,'--',y,c3,'-')


