	fprintf(stdout,"\ncompare1(aj>ai): ai=");
	for(k1=0;k1<nc;k1++) {
	  fprintf(stdout,"%g ;",a[i*nc+k1]);
	}
	fprintf(stdout,"\naj=");
	for(k1=0;k1<nc;k1++) {
	  fprintf(stdout,"%g ;",aj[k1]);
	}
	fprintf(stdout,"\n--------");
	  fprintf(stdout,"\ncompare2(aj>ai====%i): ai=",k);
	  for(k1=0;k1<nc;k1++) {
	    fprintf(stdout,"%g ;",a[i*nc+k1]);
	  }
	  fprintf(stdout,"\naj=");
	  for(k1=0;k1<nc;k1++) {
	    fprintf(stdout,"%g ;",aj[k1]);
	  }
	  fprintf(stdout,"\n^^^^^");
